{
  "name": "atelier-resleriana",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "protoc": "grpc_tools_node_protoc -I proto proto/*.proto --es_out=./src/gen --es_opt=target=ts",
    "build": "npm run protoc && tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "install:e2e": "playwright install --with-deps"
  },
  "packageManager": "npm@10.5.0",
  "dependencies": {
    "@bufbuild/protobuf": "^1.3.3",
    "@fp-ts/optic": "^0.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^3.0.0-beta.0"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "^1.3.3",
    "@playwright/test": "^1.39.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "grpc-tools": "^1.12.4",
    "jest-extended": "^4.0.2",
    "jsdom": "^24.0.0",
    "rollup-plugin-license": "^3.1.0",
    "typescript": "^5.0.2",
    "typescript-eslint-language-service": "^5.0.5",
    "vite": "^5.0.0",
    "vite-plugin-preload": "^0.4.0",
    "vitest": "^1.0.0"
  }
}
